{
    "extends": "standard",
    "env": {
        "amd": true
    },
    "rules": {
        "guard-for-in": 2,
        "no-constant-condition": 2,
        "no-empty": 2,
        "no-labels": 2,
        "no-eq-null": 2,
        "no-func-assign": 2,
        "no-native-reassign": 2,
        "no-new-func": 2,
        "no-sparse-arrays": 2,
        "no-useless-call": 2,
        "indent": 0,
        "no-unused-vars": 1,
        "shikari/sas-i18n-ems": 0,
        "no-console": 0, 
        "quotes": 0,
        "semi": 0,
        "space-before-function-paren": 0,
        "space-before-blocks": 0,
        "padded-blocks": 0,
        "no-multi-spaces": 0,
        "no-trailing-spaces": 0,
        "comma-spacing": 0,
        "spaced-comment": 0,
        "curly": 0
    }
}